@model IEnumerable<CasasRed_Nuevo3_.Models.Habilitacion>

@{
    ViewBag.Title = "Index";
}

<h2 style="margin-top:20px;">Dpt. Habilitación</h2>
<h4>Lic. Jesús</h4>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table table-striped table-responsive-md">
    <thead>
        <tr>
            <th scope="col">ID Corretaje</th>
            <th scope="col">ID Habilitacion</th>
            <th scope="col">Prueba Columna</th>
            <th scope="col">Opciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id_Corretaje)
                </td>
                <th>
                    @Html.DisplayFor(modelItem => item.Id)
                </th>
                <td>
                    @Html.DisplayFor(modelItem => item.Hbt_Apagador_doble)
                </td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = item.Id }, new { @class = "btn btn-primary" }) |
                    @Html.ActionLink("Detalles", "Details", new { id = item.Id }, new { @class = "btn btn-info" }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.Id }, new { @class = "btn btn-danger" })
                </td>
            </tr>

        }
    </tbody>
</table>




@section Scripts {
    <script>
        $(document.body).css('padding-top', '70px');
    </script>
}