@model IEnumerable<CasasRed_Nuevo3_.Models.Contaduria>

@{
    ViewBag.Title = "Index";
}

<h2 style="margin-top:20px;">Dpt. Contaduría</h2>
<h4>Lic. Arnoldo</h4>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table table-striped table-responsive-md">
    <thead>
        <tr>
            <th scope="col">ID Corretaje</th>
            <th scope="col">ID Habilitacion</th>
            <th scope="col">ID Contaduria</th>
            <th scope="col">Prueba Columna</th>
            <th scope="col">Opciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id_Corretaje)
                </td>
                <th>
                    @*@Html.DisplayFor(modelItem => item.Id_Habilitacion)*@
                </th>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @*@Html.DisplayFor(modelItem => item.Cnt_Mensualidad)*@
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>

        }
    </tbody>
</table>



@section Scripts {
    <script>
        $(document.body).css('padding-top', '70px');
    </script>
}